[user]
  name = Corentin Merot
  email = cmerot@themarqueeblink.com
[gui]
[color]
  ui = auto
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[core]
  whitespace=fix,-indent-with-non-tab,-indent-with-tab,trailing-space,cr-at-eol
[alias]
  df = !git diff --no-prefix && git diff --staged --no-prefix
  clear = reset --hard
  st = status
[github]
  user = chocopoche
